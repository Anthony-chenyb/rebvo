####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/home/odroid || mkdir -p $(INSTALL_ROOT)/home/odroid
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/home/odroid/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/home/odroid/$(QMAKE_TARGET)"

uninstall_target: FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/home/odroid/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/home/odroid/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

