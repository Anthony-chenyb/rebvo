CXX           = g++
DEFINES       = -DQT_NO_DEBUG
CFLAGS        =  -pipe -std=c++11 -Wno-unused-parameter -Wno-sign-compare -O2 -Wall -W -D_REENTRANT -fPIE $(DEFINES) $(USENE10)
CXXFLAGS      =  -pipe -std=c++11 -Wno-unused-parameter -Wno-sign-compare -O2 -Wall -W -D_REENTRANT -fPIE $(DEFINES) $(USENE10)
INCPATH       = -I/usr/lib/x86_64-linux-gnu/qt5/mkspecs/linux-g++-64 -I../rebvo -I/usr/local/include/lapackpp/ -I../Blocks -I../mtracklib -I../CommLib -I../VideoLib -I../UtilLib -I../Ne10 -I../NeonLib -I.
LINK          = g++
LFLAGS        =  -std=c++11 -Wl,-O1
LIBS          = $(SUBLIBS) -L/usr/local/lib -lX11 -lv4l2 -lm -lgd -llapack -lpthread $(USENE10)
AR            = ar cqs
RANLIB        =
TAR           = tar -cf
COMPRESS      = gzip -9f
COPY          = cp -f
SED           = sed
COPY_FILE     = cp -f
COPY_DIR      = cp -f -R
STRIP         = strip
INSTALL_FILE  = install -m 644 -p
INSTALL_DIR   = $(COPY_DIR)
INSTALL_PROGRAM = install -m 755 -p
DEL_FILE      = rm -f
SYMLINK       = ln -f -s
DEL_DIR       = rmdir
MOVE          = mv -f
CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p

####### Output directory

OBJECTS_DIR   = ./

